}


.posterImg-box {
    position: absolute;
    top: 64rpx;
    left: 32rpx;
    width: 686rpx;
    margin-bottom: 100rpx;
    z-index: 999999;
}

.posterImg {
    width: 100%;
}

.btn-create {
    margin: 32rpx;
    margin-bottom: 0;
    height: 88rpx;
    line-height: 88rpx;
    background: #e64340;
    color: #fff;
    text-align: center;

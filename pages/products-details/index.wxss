@import "/template/login/index.wxss";

page, view, image, input, textarea {
    display: block;
    box-sizing: border-box;
}

.container {
    background-color: #F2f2f2;
    min-height: 100%;
    padding-bottom: 100rpx;
}

.swiper-container {
    width: 100%;
    position: relative;
}

.swiper_box {
    width: 100%;
    height: 748rpx;
}

swiper-item image {
    width: 100%;
    display: inline-block;
    overflow: hidden;
    height: 748rpx;
}

.products-info {
    background-color: #fff;
    padding: 35rpx 0;
    margin-bottom: 24rpx;
    width: 100%;
    position: relative;
}

.products-info-fx {
    position: absolute;
    right: 35rpx;
    bottom: 60rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 130rpx;
}

.products-info-fx .item {
    width: 50rpx;
    height: 50rpx;
    right: 130rpx;
    top: 100rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    font-size: 24rpx;
    background: #fff;
    z-index: 20;
}

.products-info-fx .item image {
    width: 50rpx;
    height: 50rpx;
}

.products-info-fx .item button {
    position: absolute;
    height: 100%;
    width: 100%;
    opacity: 0;
    z-index: 99;
}

.products-info .products-title {
    box-sizing: border-box;
    padding: 0 35rpx;
    font-size: 32rpx;
    line-height: 1.4;
    color: #000;
    margin-bottom: 28rpx;
}

.products-info .products-share {
    box-sizing: border-box;
    padding: 0 35rpx;
    font-size: 25rpx;
    line-height: 1.4;
    color: #CC0000;
    margin-bottom: 28rpx;
}

.products-profile {
    display: flex;
    align-items: center;
}

.products-profile .p {
    color: #e64340;
    font-size: 20pt;
    margin-left: 30rpx;
}

.products-profile .p text {
    font-size: 12pt;
}

.products-profile .r {
    color: #ccc;
    font-size: 10pt;
    margin-left: 30rpx;
}

.row-arrow {
    width: 100vw;
    padding: 0 32rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 102rpx;
    font-size: 28rpx;
    line-height: 102rpx;
    background: #fff;
    margin-bottom: 32rpx;
}

.row-arrow image {
    width: 40rpx;
    height: 40rpx;
}

.products-des-info {
    width: 100%;
    box-sizing: border-box;
    background-color: #fff;
}

.label-title {
    font-size: 28rpx;
    color: #000000;
    padding: 30rpx;
}

.products-text {
    padding: 0 10rpx;
    font-size: 28rpx;
    color: #666666;
    line-height: 56rpx;
    margin-bottom: 30rpx;
}

.products-text image {
    width: 100%;
}

.des-imgs {
    width: 100%;
}

.des-imgs image {
    width: 100%;
}

.footer-box {
    width: 100%;
    height: 100rpx;
    background-color: #fff;
    position: fixed;
    bottom: 0;
    left: 0;
    display: flex;
    box-shadow: 0 0 1rpx 0;
    align-items: center;
}

.footer-box .contact {
    position: relative;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-left: 32rpx;
}

.footer-box .contact button {
    position: absolute;
    height: 100%;
    width: 100%;
    opacity: 0;
    z-index: 99;
}

.footer-box .contact image {
    width: 60rpx;
    height: 60rpx;
}

.footer-box .shopping-cart-btn {
    position: relative;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-left: 32rpx;
}

.footer-box .shopping-cart-btn image {
    width: 55rpx;
    height: 55rpx;
}

.fav-icon {
    width: 60rpx;
    height: 60rpx;
    margin-left: 32rpx;
    /*707070*/
}

.footer-box .shopping-cart-btn .shop-num {
    position: absolute;
    color: #fff;
    left: 32rpx;
    top: 10rpx;

    width: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    border-radius: 50%;
    background: #e64340;
    font-size: 24rpx;
}

.footer-box .join-shopping-cart {
    text-align: center;
    height: 100%;
    line-height: 100rpx;
    background-color: #ff6850;
    color: #fff;
    font-size: 34rpx;
    flex: 1;
    margin-left: 32rpx;
}

.footer-box .now-buy {
    text-align: center;
    height: 100%;
    width: 250rpx;
    line-height: 100rpx;
    background-color: #e64340;
    color: #fff;
    font-size: 34rpx;
    flex: 1;
}

.show-popup {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 999;
}

.popup-mask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.5);
    z-index: 5;
}

.popup-contents {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    background-color: #fff;
    z-index: 6;
}

.pop-products-info {
